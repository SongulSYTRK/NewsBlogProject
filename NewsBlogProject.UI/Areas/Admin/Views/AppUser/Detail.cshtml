@model GetAppUserDetailVM
@{
    ViewData["Title"] = "Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Detail</h1>

<h1>Detail</h1>

<div class="container" style="margin-top:20px">

    <div class="row">
        <div class="col-sm-12">
            <div class="box">
                <div class="box-header">
                    <h4 class="box-title">Details AppUser</h4>
                </div>
                <div class="box-body">
                    <table class="table table-hover">
                        <thead>
                            <tr>
                                <th>Image</th>
                                <th>Details</th>

                            </tr>
                        </thead>
                        <tbody>

                            <tr>
                                <td><img src="@Model.Image" width="200" /></td>
                                <td>
                                    <h4> FirstName: @Model.FirstName</h4>
                                    <h4> LastName: @Model.LastName</h4>
                                    <h4> FullName: @Model.FullName</h4>
                                    <h4> UserName: @Model.UserName</h4>
                                    <h4> Role: @Model.Role</h4>
                                    <h4> CreateDate: @Model.CreateDate</h4>

                                </td>

                            </tr>
                            }
                        </tbody>
                    </table>
                </div>
                <div class="box-footer">
                    <a asp-action="list" class="btn btn-block btn-primary">Back To List</a>
                </div>
            </div>
        </div>
    </div>
</div>
